<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  
    <display-name>ItmsV1</display-name>

	<servlet>
		<servlet-name>Install</servlet-name>
		<servlet-class>com.xiangxun.atms.common.install.InstallServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Install</servlet-name>
		<url-pattern>/install/install_setup.svl</url-pattern>
	</servlet-mapping>

    <servlet>
		<servlet-name>Runing</servlet-name>
		<servlet-class>com.xiangxun.atms.common.install.RuningServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Runing</servlet-name>
		<url-pattern>/install/runing_setup.svl</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>install.html</welcome-file>
	</welcome-file-list>

</web-app>